                        <div class="prt_01 mb-2"><span class="text-success bg-light-success rounded px-2 py-1">Women's Suit</span></div>
                        <div class="prt_02 mb-3">
                            <h2 class="ft-bold mb-1">@Model.Product.Name</h2>
                            <div class="text-left">
                                <div class="star-rating align-items-center d-flex justify-content-left mb-1 p-0">
                                    <i class="fas fa-star filled"></i>
                                    <i class="fas fa-star filled"></i>
                                    <i class="fas fa-star filled"></i>
                                    <i class="fas fa-star filled"></i>
                                    <i class="fas fa-star"></i>
                                    <span class="small">(412 Reviews)</span>
                                </div>
                                @{
                                    var price = Model.Product.Price - (Model.Product.Price / 100) * Model.Product.Discount;
                                }
                                <div class="elis_rty"><span class="ft-medium text-muted line-through fs-md mr-2">$@Model.Product.Price</span><span class="ft-bold theme-cl fs-lg">$@price</span></div>
                            </div>
                        </div>

                        <div class="prt_03 mb-4">
